<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/" />

  <remote  name="github"
           fetch="git://github.com/" />

  <remote  name="gtab2"
           fetch="git://github.com/SamsungGalaxyTab2-AOSP" />

  <default revision="refs/tags/android-5.1.1_r9"
           remote="aosp"
           sync-c="true"
           sync-j="4" />

  <project path="build" name="platform/build" groups="pdk,tradefed" >
